echo "[hcf] start onboard mavlink"
#mavlink stop-all
usleep 10000
mavlink start -m custom -d /dev/ttyS1 -b 230400
mavlink stream -d /dev/ttyS1 -s HIGHRES_IMU -r 200
#mavlink stream -d /dev/ttyS1 -s ATTITUDE_R -r 30
mavlink stream -d /dev/ttyS1 -s FORCE_SETPOINT -r 30
mavlink stream -d /dev/ttyS1 -s ATTITUDE -r 100
if [ $GPS == yes ]
than
	mavlink stream -d /dev/ttyS1 -s GPS_RAW_INT -r 20
fi
usleep 10000

