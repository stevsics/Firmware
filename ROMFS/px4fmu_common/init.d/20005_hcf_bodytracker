#!nsh
#
# IMU mode for tests
#
echo "[init] 20005_hcf_bodytracker"
set USE_IO no
set MIXER skip
set HCF yes
set HCF_VICON no
set VEHICLE_TYPE hcf_bodytracker

# higher baudrate for lairdtech on serial 1
set MAVLINK_FLAGS "-r 1000 -d /dev/ttyS0 -b 115200"

# set EXIT_ON_END because we set a custom MAVLINK_FLAGS
set EXIT_ON_END yes